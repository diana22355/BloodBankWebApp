<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>

<display-name>ICBlood Bank</display-name>

<description>Capstone Project </description>

<servlet>
<servlet-name>LoginUser</servlet-name>
<servlet-class>bloodbank.LoginUserServlet</servlet-class>
</servlet>

<servlet>
<servlet-name>CreateUser</servlet-name>
<servlet-class>bloodbank.CreateUserServlet</servlet-class>
</servlet>

<servlet>
<servlet-name>CloseSession</servlet-name>
<servlet-class>bloodbank.CloseSession</servlet-class>
</servlet>

<servlet>
<servlet-name>MenuUser</servlet-name>
<servlet-class>bloodbank.OperationUserServlet</servlet-class>
</servlet>

<servlet>
<servlet-name>UpdateAppointment</servlet-name>
<servlet-class>bloodbank.UpdateAppointment</servlet-class>
</servlet>

<servlet>
<servlet-name>UserUpdate</servlet-name>
<servlet-class>bloodbank.UpdateUserServlet</servlet-class>
</servlet>

<servlet>
<servlet-name>LoginAdmin</servlet-name>
<servlet-class>bloodbank.LoginAdminServlet</servlet-class>
</servlet>

<servlet>
<servlet-name>MenuAdmin</servlet-name>
<servlet-class>bloodbank.OperationAdminServlet</servlet-class>
</servlet>

<servlet>
<servlet-name>CreateDoctor</servlet-name>
<servlet-class>bloodbank.CreateDoctorServlet</servlet-class>
</servlet>

<servlet>
<servlet-name>DeleteDoctor</servlet-name>
<servlet-class>bloodbank.DeleteDoctorServlet</servlet-class>
</servlet>

<servlet>
<servlet-name>ViewEditAppointmentAdmin</servlet-name>
<servlet-class>bloodbank.ViewEditUserAppointmentAdminServlet</servlet-class>
</servlet>

<servlet>
<servlet-name>ViewAppointmentAdmin</servlet-name>
<servlet-class>bloodbank.ViewUserAppointmentAdminServlet</servlet-class>
</servlet>

<servlet>
<servlet-name>ViewAllAppointmentAdminLoc</servlet-name>
<servlet-class>bloodbank.ViewAllAppointmentLocServlet</servlet-class>
</servlet>

<servlet>
<servlet-name>ViewSpecAppointmentAdmin</servlet-name>
<servlet-class>bloodbank.ViewSpecAppointmentServlet</servlet-class>
</servlet>

<servlet>
<servlet-name>EditAppointmentInfo</servlet-name>
<servlet-class>bloodbank.EditAppointmentInfoServlet</servlet-class>
</servlet>

<servlet>
<servlet-name>DeleteUserAppointmentAdmin</servlet-name>
<servlet-class>bloodbank.ConfirmUserAppointmentServlet</servlet-class>
</servlet>

<servlet>
<servlet-name>AddBloodDonation</servlet-name>
<servlet-class>bloodbank.AddBloodDonationServlet</servlet-class>
</servlet>

<servlet>
<servlet-name>ViewEditRequests</servlet-name>
<servlet-class>bloodbank.ViewEditRequests</servlet-class>
</servlet>

<servlet>
<servlet-name>FilterDoctorRequest</servlet-name>
<servlet-class>bloodbank.FilterEditDoctorRequests</servlet-class>
</servlet>

<servlet>
<servlet-name>ModifyRequest</servlet-name>
<servlet-class>bloodbank.ModifyRequestServlet</servlet-class>
</servlet>


<servlet-mapping>
<servlet-name>LoginUser</servlet-name>
<url-pattern>/LoginUser</url-pattern>
</servlet-mapping>

<servlet-mapping>
<servlet-name>CreateUser</servlet-name>
<url-pattern>/CreateUser</url-pattern>
</servlet-mapping>

<servlet-mapping>
<servlet-name>CloseSession</servlet-name>
<url-pattern>/CloseSession</url-pattern>
</servlet-mapping>

<servlet-mapping>
<servlet-name>MenuUser</servlet-name>
<url-pattern>/MenuUser</url-pattern>
</servlet-mapping>

<servlet-mapping>
<servlet-name>UpdateAppointment</servlet-name>
<url-pattern>/UpdateAppointment</url-pattern>
</servlet-mapping>

<servlet-mapping>
<servlet-name>UserUpdate</servlet-name>
<url-pattern>/SendUserUpdate</url-pattern>
</servlet-mapping>

<servlet-mapping>
<servlet-name>LoginAdmin</servlet-name>
<url-pattern>/LoginAdmin</url-pattern>
</servlet-mapping>

<servlet-mapping>
<servlet-name>MenuAdmin</servlet-name>
<url-pattern>/MenuAdmin</url-pattern>
</servlet-mapping>

<servlet-mapping>
<servlet-name>CreateDoctor</servlet-name>
<url-pattern>/CreateDoctor</url-pattern>
</servlet-mapping>

<servlet-mapping>
<servlet-name>DeleteDoctor</servlet-name>
<url-pattern>/DeleteDoctor</url-pattern>
</servlet-mapping>

<servlet-mapping>
<servlet-name>ViewEditAppointmentAdmin</servlet-name>
<url-pattern>/ViewEditAppointmentAdmin</url-pattern>
</servlet-mapping>

<servlet-mapping>
<servlet-name>ViewAppointmentAdmin</servlet-name>
<url-pattern>/ViewAppointmentAdmin</url-pattern>
</servlet-mapping>

<servlet-mapping>
<servlet-name>ViewAllAppointmentAdminLoc</servlet-name>
<url-pattern>/ViewAppointmentLocation</url-pattern>
</servlet-mapping>

<servlet-mapping>
<servlet-name>ViewSpecAppointmentAdmin</servlet-name>
<url-pattern>/SpecificUserAppointment</url-pattern>
</servlet-mapping>

<servlet-mapping>
<servlet-name>EditAppointmentInfo</servlet-name>
<url-pattern>/EditUserAppointment</url-pattern>
</servlet-mapping>

<servlet-mapping>
<servlet-name>DeleteUserAppointmentAdmin</servlet-name>
<url-pattern>/DeleteAppointmentUser</url-pattern>
</servlet-mapping>

<servlet-mapping>
<servlet-name>AddBloodDonation</servlet-name>
<url-pattern>/CreateBloodDonation</url-pattern>
</servlet-mapping>

<servlet-mapping>
<servlet-name>ViewEditRequests</servlet-name>
<url-pattern>/ViewEditDoctorRequests</url-pattern>
</servlet-mapping>

<servlet-mapping>
<servlet-name>FilterDoctorRequest</servlet-name>
<url-pattern>/FilterDoctorRequest</url-pattern>
</servlet-mapping>

<servlet-mapping>
<servlet-name>ModifyRequest</servlet-name>
<url-pattern>/ModifyRequest</url-pattern>
</servlet-mapping>

</web-app>